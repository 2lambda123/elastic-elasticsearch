{
  "settings": {
    "auto_expand_replicas": "0-1"
  },
  "mappings": {
    "_doc" : {
      "dynamic": false,
      "_meta": {
        "version": "8.99.99",
        "system-index-mappings-version": ${fleet.version}
      },
      "properties": {
        "server": {
          "properties": {
            "id": {
              "type": "keyword"
            },
            "version": {
              "type": "keyword"
            }
          }
        },
        "@timestamp": {
          "type": "date"
        }
      }
    }
  }
}
