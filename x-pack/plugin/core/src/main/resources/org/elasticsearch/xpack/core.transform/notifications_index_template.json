{
  "order" : 0,
  "version" : ${xpack.transform.version.id},
  "index_patterns" : [
    ".transform-notifications-*"
  ],
  "aliases": {
    ".transform-notifications-read": {
      "hidden": true
    }
  },
  "settings" : {
    "index" : {
      "number_of_shards" : "1",
      "auto_expand_replicas" : "0-1",
      "hidden": true
    }
  },
  "mappings" : {
    "_doc": {
      "_meta" : {
        "version" : "${xpack.transform.version}"
      },
      "dynamic" : "false",
      "properties" : {
        "transform_id": {
          "type": "keyword"
        },
        "level": {
          "type": "keyword"
        },
        "message": {
          "type": "text",
          "fields": {
            "raw": {
              "type": "keyword"
            }
          }
        },
        "timestamp": {
          "type": "date"
        },
        "node_name": {
          "type": "keyword"
        }
      }
    }
  }
}
