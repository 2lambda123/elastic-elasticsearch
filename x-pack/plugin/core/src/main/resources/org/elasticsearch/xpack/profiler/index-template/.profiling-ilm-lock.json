{
  "index_patterns": [
    ".profiling-ilm-lock*"
  ],
  "template": {
    "settings": {
      "index": {
        "hidden": true
      }
    },
    "mappings": {
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "epoch_second"
        },
        "phase": {
          "type": "keyword"
        }
      }
    }
  },
  "priority": 100,
  "_meta": {
    "description": "Index template for .profiling-ilm-lock"
  },
  "version": ${xpack.profiling.template.version}
}
