{
  "template": {
    "mappings": {
      "dynamic_templates": [
        {
          "message": {
            "path_match": "message",
            "mapping": { "type": "match_only_text" }
          }
        },
        {
          "log_level": {
            "path_match": "log.level",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "log_logger": {
            "path_match": "log.logger",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "trace_id": {
            "path_match": "trace.id",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "span_id": {
            "path_match": "span.id",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "transaction_id": {
            "path_match": "transaction.id",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "service_name": {
            "path_match": "service.name",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "service_version": {
            "path_match": "service.version",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "service_environment": {
            "path_match": "service.environment",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "process_pid": {
            "path_match": "process.pid",
            "mapping": { "type": "long" }
          }
        },
        {
          "process_thread_name": {
            "path_match": "process.thread.name",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "error_type": {
            "path_match": "error.type",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "error_message": {
            "path_match": "error.message",
            "mapping": { "type": "match_only_text" }
          }
        },
        {
          "event_dataset": {
            "path_match": "event.dataset",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "cloud_provider": {
            "path_match": "cloud.provider",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "cloud_availability_zone": {
            "path_match": "cloud.availability_zone",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "cloud_region": {
            "path_match": "cloud.region",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "host_hostname": {
            "path_match": "host.hostname",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "host_name": {
            "path_match": "host.name",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "container_id": {
            "path_match": "container.id",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "container_name": {
            "path_match": "container.name",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "orchestrator_namespace": {
            "path_match": "orchestrator.namespace",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "orchestrator_id": {
            "path_match": "orchestrator.id",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "orchestrator_resource_id": {
            "path_match": "orchestrator.resource.id",
            "mapping": { "type": "keyword" }
          }
        },
        {
          "orchestrator_resource_name": {
            "path_match": "orchestrator.resource.name",
            "mapping": { "type": "keyword" }
          }
        }
      ],
      "dynamic": "runtime",
      "date_detection": false,
      "properties": {
        "@timestamp": {
          "type": "date",
          "ignore_malformed": false
        },
        "data_stream": {
          "properties": {
            "type": {
              "type": "constant_keyword",
              "value": "logs"
            },
            "dataset": {
              "type": "constant_keyword"
            },
            "namespace": {
              "type": "constant_keyword"
            }
          }
        },
        "log": {
          "type": "object"
        },
        "error": {
          "type": "object"
        },
        "event": {
          "type": "object"
        },
        "trace": {
          "type": "object"
        },
        "transaction": {
          "type": "object"
        },
        "span": {
          "type": "object"
        },
        "cloud": {
          "type": "object"
        },
        "container": {
          "type": "object"
        },
        "host": {
          "type": "object"
        },
        "orchestrator": {
          "properties": {
            "resource": {
              "type": "object"
            }
          }
        },
        "process": {
          "properties": {
            "thread": {
              "type": "object"
            }
          }
        },
        "service": {
          "type": "object"
        }
      }
    }
  },
  "_meta": {
    "description": "default mappings for the logs index template installed by x-pack",
    "managed": true
  },
  "version": ${xpack.stack.template.version}
}
