{
  "security.clear_security_cache":{
    "documentation":{
      "url":"https://www.elastic.co/guide/en/elasticsearch/reference/current/security-api-clear-security-cache.html",
      "description":"Clear a subset or all entries from the specified security cache."
    },
    "stability":"stable",
    "url":{
      "paths":[
        {
          "path":"/_security/{cache_name}/{keys}/_clear_cache",
          "methods":[
            "POST"
          ],
          "parts":{
            "cache_name":{
              "type":"string",
              "description":"Name of the target cache to be cleared"
            },
            "keys":{
              "type":"list",
              "description":"A comma-separated list of keys for cache entries"
            }
          }
        }
      ]
    },
    "params":{}
  }
}
