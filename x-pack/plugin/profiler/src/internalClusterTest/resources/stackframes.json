{
  "settings": {
    "index": {
      "number_of_shards": "16",
      "refresh_interval": "10s",
      "max_result_window": 100000
    }
  },
  "mappings": {
    "_doc": {
      "_source": {
        "enabled": true
      },
      "properties": {
        "ecs.version": {
          "type": "keyword",
          "index": true,
          "doc_values": false,
          "store": false
        },
        "Stackframe.line.number": {
          "type": "integer",
          "index": false,
          "doc_values": false,
          "store": false
        },
        "Stackframe.file.name": {
          "type": "keyword",
          "index": false,
          "doc_values": false,
          "store": false
        },
        "Stackframe.function.name": {
          "type": "keyword",
          "index": false,
          "doc_values": false,
          "store": false
        }
      }
    }
  }
}
