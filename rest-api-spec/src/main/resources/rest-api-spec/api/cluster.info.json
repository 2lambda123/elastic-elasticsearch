{
  "cluster.info": {
      "documentation": {
          "url": "https://www.elastic.co/guide/en/elasticsearch/reference/master/cluster-info.html",
          "description": "Returns statistical information about different metrics in the cluster."
      },
      "stability": "stable",
      "visibility": "public",
      "headers": {
          "accept": ["application/json"]
      },
      "url": {
          "paths": [
              {
                  "path": "/_info/{metric}",
                  "methods": [
                      "GET"
                  ],
                  "parts":{
                      "metric":{
                          "type":"list",
                          "options":[
                              "_all",
                              "http"
                          ],
                          "description":"Limit the information returned to the specified metrics"
                      }
                  }
              }
          ]
      },
      "params": {}
  }
}
